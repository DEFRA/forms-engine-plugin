    "@mdx-js/react": "^3.1.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/atob": "^2.1.4",
    "@types/btoa": "^1.2.5",
    "@types/convict": "^6.1.6",
